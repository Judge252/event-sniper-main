# API Configuration
VITE_API_URL=http://localhost:8000
VITE_API_VERSION=v1

# Authentication
VITE_AUTH_ENABLED=true
VITE_AUTH_TOKEN_KEY=webook_auth_token

# Feature Flags
VITE_ENABLE_PROXY_ROTATION=false
VITE_ENABLE_AUTO_PURCHASE=false
VITE_ENABLE_NOTIFICATIONS=true

# Notification Settings
VITE_TELEGRAM_BOT_TOKEN=your_telegram_bot_token
VITE_TELEGRAM_CHAT_ID=your_telegram_chat_id
VITE_EMAIL_SERVICE_ENABLED=false
VITE_EMAIL_SERVICE_ADDRESS=your_email@example.com

# Proxy Settings
VITE_PROXY_LIST=proxy1.example.com:8080,proxy2.example.com:8080
VITE_PROXY_ROTATION_INTERVAL=5

# Database
VITE_DB_TYPE=sqlite
VITE_DB_PATH=./data/webook.db

# Security
VITE_ENCRYPTION_KEY=your_encryption_key
VITE_SESSION_TIMEOUT=3600 